{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-filename-extension": [1,{"extensions":[".js",".jsx"]}],
        "rect/prop-types":"off",
        "reactjsx-props-no-spreading":["error",{
          "html": "ignore",
          "custom": "ignore",
          "exceptions": [""]
        }],
        "react/react-in-jsx-scope": "off"
    }
}
